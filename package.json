{
  "name": "crls",
  "version": "1.0.0",
  "description": "Recursos y assets para el portafolio de C.A. Esteve",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "dev": "npx @tailwindcss/cli -i ./src/input.css -o ./assets/css/theme.css --watch",
    "prod": "npx @tailwindcss/cli -i ./src/input.css -o ./assets/css/theme.css --minify",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production"
  },
  "keywords": [
    "portafolio",
    "dev"
  ],
  "author": "Carles A. Esteve Albiach",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.49",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "@splidejs/splide": "^4.1.4",
    "@splidejs/splide-extension-auto-scroll": "^0.5.3",
    "@tailwindcss/cli": "^4.1.17",
    "body-parser": "^1.20.3",
    "postcss": "^8.4.41",
    "postcss-nested": "^6.2.0",
    "tailwindcss": "^3.4.18",
    "three": "^0.167.1"
  }
}
